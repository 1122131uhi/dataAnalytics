      ],
      "execution_count": 16,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "S646OaU0R8UQ",
        "outputId": "098eaa86-90f8-477e-cff0-89fd0c9ea796",
